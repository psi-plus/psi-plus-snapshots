/* Last message correction */
QWidget#bottomFrame>QWidget>QTextEdit[correction="true"] {
    background-color: rgb(0,51,0);
}

QFrame {
    background-color: #b4ccff;
    border: 1px solid blue;
    border-radius: 6px;
    padding: 2px;
}

QToolButton {
    background-color: #e9edff;
    border: 1px solid blue;
    border-radius: 4px;
    padding: 2px;
}

QToolButton:hover {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #e9edff, stop: 1 black);
}

QToolButton:pressed {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 black, stop: 1 #e9edff);
}

QToolButton:checked {
    border-top: 2px solid blue;
    border-left: 2px solid blue;
    padding-bottom: 1px;
    padding-right: 1px;
    background-color: #aebece;
}

QLabel {
    background-color: #b5e3ff;
}

QLabel#lb_status {
    background-color: #b4ccff;
    border: 0px;
    border-radius: 0px;
    padding: 0px;
}

QLabel#lb_client {
    background-color: #b4ccff;
    border: 0px;
    border-radius: 0px;
    padding: 0px;
}

QToolBar {
    background-color: #b5e3ff;
    border: 1px solid blue;
    border-radius: 4px;
    padding: 2px;
}

QTextEdit {
    background-color: #eefeff;
}

QLineEdit {
    background-color: #e9edff;
    border: 1px solid blue;
    border-radius: 6px;
    padding: 2px;
}

QPushButton {
    background-color: #e9edff;
    border: 1px solid blue;
    border-radius: 4px;
}

QPushButton#pb_topic {
    border-radius: 0px;
}

QPushButton:hover:pressed {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 black, stop: 1 #e9edff);
}

QPushButton:hover {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #e9edff, stop: 1 black);
}

Q3ListView * {
    background-color: #e9edff;
}

QFrame#log * {
    background-color: #eefeff;
}

QTabBar::tab {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #E1E1E1, stop: 0.4 #DDDDDD, stop: 0.5 #D8D8D8, stop: 1.0 #D3D3D3);
    border: 2px solid #C4C4C3;
    border-bottom-color: #C2C7CB;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    min-width: 8ex;
    padding: 2px;
}

QTabBar::tab:selected, QTabBar::tab:hover {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #fafafa, stop: 0.4 #f4f4f4, stop: 0.5 #e7e7e7, stop: 1.0 #fafafa);
}

QTabBar::tab:selected {
    border-color: #9B9B9B;
    border-bottom-color: #C2C7CB;
}

QToolTip {
    background-color: #a9bcff;
    border: 2px solid blue;
    padding: 2px;
    border-radius: 4px;
}

QMenu {
    background-color: #a9bcff;
    border: 1px solid blue;
}

QMenu::item {
    background-color: #a9bcff;
    border: 1px solid transparent;
    padding: 2px 25px 2px 20px;
}

QMenu::item:selected {
    background-color: #e9edff;
    border-color: darkblue;
}

QMenu::separator {
    height: 2px;
    background: lightblue;
}

QMenu::item:checked {
    background-color: #e9edff;
}

TabDlg {
    background-color: #b4ccff;
}
