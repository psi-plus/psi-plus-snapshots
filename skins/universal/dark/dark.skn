        <options.ui.chat.css type="QString">
TabDlg {
    background-color: #212121;
}

/* common frame */
QFrame {
    color: #bcbcbc;
    background-color: #212121;
    border: 1px solid #5b5b5b;
    padding: 0px;
}

/* top frame */
QFrame#topFrame {
    margin: 0px;
    margin-bottom: 2px;
    border: 1px solid #2d2d2d;
}

/* topic button */
QPushButton#pb_topic {
    margin: 0px;
    height: 16px;
    color: #bcbcbc;
    background-color: #5b5b5b;
    font-weight: bold;
    border: 1px solid #5b5b5b;
    border-radius: 3px;
    padding: 2px;
}

QPushButton#pb_topic:hover {
    color: #bcbcbc;
    background-color: #2d2d2d;
}

QPushButton#pb_topic:pressed {
    background-color: #212121;
}

/* topic dialog */
QWidget#pte_topic {
    color: #bcbcbc;
    background-color: #212121;
    border: 1px inset #5b5b5b;
    border-radius: 3px;
}

/* status icon */
QLabel#lb_status {
    border: 0px;
    background: #212121;
}

/* topic / JID */
ActionLineEdit#le_topic,#le_jid {
    color: #bcbcbc;
    background-color: #2d2d2d;
    font-weight: bold;
    border: 1px solid #5b5b5b;
    border-radius: 3px;
    padding: 2px;
}

/* JID - dropdown menu */
QComboBox::drop-down#le_jid {
    height: 20px;
    border: 1px solid #5b5b5b;
    background-color: #212121;
}

QComboBox::down-arrow#le_jid {
    width: 9px;
}

QComboBox#le_jid QAbstractItemView {
    color: #bcbcbc;
    selection-background-color: #2d2d2d;
    background-color: #212121;
    show-decoration-selected: 0;
    border: 1px outset #5b5b5b;
}

QComboBox::focus#le_jid QAbstractItemView {
    border-color: black;
    border: 1px;
}

/* bookmark dialog box */
QDialog {
    background-color: #212121;
}

QDialog>QFrame {
    color: #bcbcbc;
    background: #2d2d2d;
}

QDialog>QLineEdit {
    color: #bcbcbc;
    background: #2d2d2d;
    border: 1px inset #5b5b5b;
    border-radius: 3px;
}

QDialog>QCheckBox {
    background: #212121;
}

/* client icon */
QLabel#lb_client {
    border: 0px;
    background: #212121;
}

/* character counter */
QLabel#lb_count {
    color: #bcbcbc;
    background: #212121;
    background-color: #2d2d2d;
    font-weight: bold;
    border: 1px solid #5b5b5b;
    border-radius: 3px;
    padding: 2px;
}

/* account name */
QLabel#lb_ident {
    color: #bcbcbc;
    background: #212121;
    background-color: #2d2d2d;
    font-weight: bold;
    border: 1px solid #5b5b5b;
    border-radius: 3px;
    padding: 2px;
}

/* chatlog */
QFrame#log * {
    color: #bcbcbc;
    background-color: #2d2d2d;
    border: 1px inset #5b5b5b;
    border-radius: 3px;
    padding: 2px;
}

/* conference roster */
QFrame#lv_users {
    border: 1px upset #5b5b5b;
    border-radius: 3px;
    padding: 2px;
    background-color: #212121;
    selection-color: #bcbcbc;
    selection-background-color: #2d2d2d;
}

/* bottom frame */
QFrame#bottomFrame {
    margin: 0px;
    border: 1px;
}

/* command input field */
QWidget#mini_prompt {
    margin-top: -8px;
    margin-left: 1px;
    margin-right: -5px;
    color: #bcbcbc;
    background: #2d2d2d;
}

/* input line */
QWidget#bottomFrame>QWidget>QTextEdit {
    margin: 0px;
    color: #bcbcbc;
    background-color: #2d2d2d;
    border: 1px inset #5b5b5b;
    border-radius: 3px;
}

/* send message button */
QPushButton#pb_send {
    margin: 0px;
    background-color: #2d2d2d;
    border: 1px outset #5b5b5b;
    border-radius: 3px;
}

QPushButton#pb_send:hover {
    background-color: #5b5b5b;
}

QPushButton#pb_send:hover:pressed {
    background-color: #212121;
}

/* avatar */
QLabel#avatar {
    margin: 0px;
    background: #212121;
}

/* central bar */
QToolBar {
    background-color: #212121;
    border: 1px solid #5b5b5b;
    border-radius: 4px;
    padding: 2px;
}

/* central bar buttons and more */
QToolButton {
    margin-left: 0px;
    margin-right: 0px;
    background-color: #2d2d2d;
    border: 1px outset #5b5b5b;
    border-radius: 3px;
    padding: 0px;
}

QToolButton:hover {
    background-color: #5b5b5b;
}

QToolButton:pressed {
    background-color: #212121;
}

QToolButton:checked {
    background-color: #000000;
    border: 1px solid #5b5b5b;
}

QToolButton::menu-indicator {
    image: none;
}

/* vertical splitter */
QSplitter::handle:vertical {
    background-color: #2d2d2d;
    border-left: 1px outset #212121;
    border-right: 1px outset #212121;
}

QSplitter::handle:vertical:hover {
    background: #5b5b5b;
}

QSplitter::handle:vertical:pressed {
    background-color: #2d2d2d;
}

/* horizontal splitter */
QSplitter::handle:horizontal {
    background-color: #212121;
    border: 1px;
}

QSplitter::handle:horizontal:hover {
    background-color: #2d2d2d;
    border-left: 1px outset #212121;
    border-right: 1px outset #212121;
}

QSplitter::handle:horizontal:pressed {
    background-color: #2d2d2d;
}

/* tab bar */
PsiTabBar {
    background: #212121;
    background-color: #212121;
    border: 0px solid #212121;
    qproperty-expanding: false;
}

/* Try to place tabs at left side of tab bar */
QStyleOptionTab {
    position: begining;
}

/* tab bar - main font color */
TabDlg {
    color: #bcbcbc;
    background-color: #212121;
}

/* Inactive tab */
QTabBar::tab {
    border: 2px solid #5b5b5b;
    border-bottom-color: #212121;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    min-width: 8px;
    padding: 2px;
}

QTabBar::tab:selected, QTabBar::tab:hover {
    color: #bcbcbc;
    border: 2px outset #5b5b5b;
    border-bottom-color: #212121;
    background: #212121;
    background-color: #212121;
}

QTabBar::tab:!selected {
    color: #5b5b5b;
    border: 2px outset #2d2d2d;
    border-bottom-color: #212121;
    background-color: #2d2d2d;
}

/* conference settings window - tabs, frame, buttons, list of JIDs */
QWidget#MUCConfig>QTabBar::tab {
    color: #bcbcbc;
    margin-bottom: 3px;
    width: 100%;
    background: #212121;
    border: 1px outset #5b5b5b;
    border-radius: 3px;
}

QWidget#MUCConfig>QTabBar::tab:selected {
    font: bold;
    background-color: #212121;
}

QWidget#MUCConfig>QTabBar::tab:hover {
    background-color: #2d2d2d;
}

QWidget#MUCConfig>QWidget {
    color: #bcbcbc;
    background-color: #212121;
}

QWidget#MUCConfig>QPushButton {
    color: #bcbcbc;
    height: 25px;
    background-color: #212121;
    border: 1px outset #5b5b5b;
    border-radius: 3px;
    padding-left: 8px;
    padding-right: 8px;
}

QWidget#MUCConfig>QPushButton:hover {
    background-color: #2d2d2d;
}

QWidget#MUCConfig>QPushButton:pressed {
    background-color: #5b5b5b;
}

QFrame#tv_affiliations {
    color: #bcbcbc;
    background: #212121;
    selection-background-color: #2d2d2d;
    border: 1px inset #5b5b5b;
    border-radius: 3px;
    padding: 2px;
}

/* conference settings window - filter */
QLabel#label {
    color: #bcbcbc;
    background: #212121;
}

QLineEdit#le_filter {
    color: #bcbcbc;
    background: #212121;
    border: 1px inset #5b5b5b;
    border-radius: 3px;
    padding: 2px;
}

/* conference settings window - getting settings */
QFrame#lb_general_message {
    color: #bcbcbc;
    font-size: 18px;
    background-color: #212121;
}

/* conference settings window - settings, external and internal frame */
QWidget#tab_general QWidget {
    background: #212121;
}

/* conference settings window - settings, internal frame */
QStackedWidget#sw_general {
    border: 1px inset #5b5b5b;
    border-radius: 3px;
    padding: 1px;
}

/* conference settings window - button to delete the conference */
QWidget#tab_general QPushButton#pb_destroy {
    background: #212121;
    border: 1px outset #5b5b5b;
    border-radius: 3px;
    padding-left: 8px;
    padding-right: 8px;
}

QWidget#tab_general QPushButton#pb_destroy:hover {
    font-weight: bold;
    background-color: #2d2d2d;
}

QWidget#tab_general QPushButton#pb_destroy:pressed {
    background-color: #5d5d5d;
}

/* conference settings window - settings, option names */
QWidget#tab_general QLabel {
    background: #212121;
}

/* conference settings window - settings, input fields */
QWidget#tab_general QLineEdit {
    color: #bcbcbc;
    background: #212121;
    border: 1px inset #5b5b5b;
    border-radius: 3px;
    padding: 2px;
}

/* conference settings window - settings, drop-down menus */
QWidget#tab_general QComboBox {
    color: #bcbcbc;
    background: #212121;
    border: 1px outset #5b5b5b;
    border-radius: 3px;
    padding: 2px;
}

QWidget#tab_general QComboBox::drop-down {
    height: 20px;
    border-left: 1px solid #5b5b5b;
    background-color: #2d2d2d;
}

QWidget#tab_general QComboBox::down-arrow {
    image: none;
}

QWidget#tab_general QComboBox QAbstractItemView {
    color: #bcbcbc;
    selection-background-color: #2d2d2d;
    background-color: #212121;
    border: 1px outset #5b5b5b;
}

/* scroll bars */
QScrollBar {
    background-color: #212121;
    border: 1px inset #444444;
    border-radius: 7px;
}

QScrollBar:vertical {
    width: 16px;
    margin-left: 1px;
    margin-right: 1px;
}

QScrollBar:horizontal {
    height: 16px;
    margin-top: 1px;
    margin-bottom: 1px;
}

QScrollBar::handle {
    background-color: #2d2d2d;
    border: 1px outset #444444;
    border-radius: 6px;
}

QScrollBar::handle:hover {
    background-color: #5b5b5b;
    border: 1px outset #5b5b5b;
}

/* scroll bar, vertical arrows */
QScrollBar::sub-line:vertical, QScrollBar::add-line:vertical {
    height: 0px;
}

/* scroll bar, horizontal arrows */
QScrollBar::sub-line:horizontal, QScrollBar::add-line:horizontal {
    width: 0px;
}

/* scroll bar - background */
QScrollBar::sub-page, QScrollBar::add-page {
    background: none;
}

Q3ListView * {
    background-color: #212121;
}

/* tooltips */
QToolTip {
    color: #bcbcbc;
    background-color: #212121;
    border: 1px solid #5b5b5b;
}

/* dialogs for warning clearing chat and closing when unread message */
QMessageBox {
    color: #bcbcbc;
    background-color: #212121;
}

/* dialog box buttons */
QPushButton {
    color: #bcbcbc;
    height: 25px;
    background-color: #212121;
    border: 1px outset #5b5b5b;
    border-radius: 3px;
    padding-left: 8px;
    padding-right: 8px;
}

QPushButton:hover {
    background-color: #5b5b5b;
}

QPushButton:pressed {
    background-color: #212121;
    border: 1px inset #5b5b5b;
}

QCheckBox {
    color: #bcbcbc;
}

QLabel {
    color: #bcbcbc;
}

/* search bar */
TypeAheadFindBar {
    margin: 0px;
    color: #bcbcbc;
    padding: 2px;
    background: #212121;
    border: 1px outset #5b5b5b;
    border-radius: 3px;
}

TypeAheadFindBar>QLineEdit {
    margin-left: 1px;
    margin-right: 1px;
    color: #bcbcbc;
    background: #2d2d2d;
    border: 1px inset #5b5b5b;
    border-radius: 3px;
}

TypeAheadFindBar>QCheckBox {
    margin-left: 3px;
    color: #bcbcbc;
    background-color: #2d2d2d;
}

TypeAheadFindBar>QToolButton, TypeAheadFindBar>QToolButton:hover {
    color: #bcbcbc;
    background-color: #444444;
    border: 0px;
}

/* Kick and ban Reason Editor */
MUCReasonsEditor>QWidget {
    color: #bcbcbc;
    background-color: #212121;
}

QLineEdit#txtReason {
    color: #bcbcbc;
    background: #2d2d2d;
    border: 1px inset #5b5b5b;
    border-radius: 3px;
}

QListWidget#lstReasons {
    background: #212121;
    selection-background-color: #2d2d2d;
    border: 1px inset #5b5b5b;
    border-radius: 3px;
}

MUCReasonsEditor>QPushButton#btnOk, MUCReasonsEditor>QPushButton#btnCancel {
    color: #bcbcbc;
    height: 25px;
    background: #212121;
    border: 1px outset #5b5b5b;
    border-radius: 3px;
    padding-left: 8px;
    padding-right: 8px;
}

MUCReasonsEditor>QPushButton:hover#btnOk, MUCReasonsEditor>QPushButton:hover#btnCancel {
    background-color: #2d2d2d;
}

MUCReasonsEditor>QPushButton:pressed#btnOk, MUCReasonsEditor>QPushButton:pressed#btnCancel {
    background-color: #444444;
    border: 1px inset #5b5b5b;
}

/* quick insert/send template editor */
SendButtonTemplatesEditor>QWidget {
    color: #bcbcbc;
    background-color: #212121;
}

QLineEdit#txtTemplate {
    color: #bcbcbc;
    background: #212121;
    border: 1px inset #5b5b5b;
    border-radius: 3px;
}

QListWidget#lstTemplates {
    background-color: #212121;
    selection-background-color: #2d2d2d;
    border: 1px inset #5b5b5b;
    border-radius: 3px;
    padding: 2px;
}

SendButtonTemplatesEditor>QPushButton#btnOk, SendButtonTemplatesEditor>QPushButton#btnCancel {
    color: black;
    height: 25px;
    background: #212121;
    border: 1px outset #5b5b5b;
    border-radius: 3px;
    padding-left: 8px;
    padding-right: 8px;
}

SendButtonTemplatesEditor>QPushButton:hover#btnOk, SendButtonTemplatesEditor>QPushButton:hover#btnCancel {
    background-color: #2d2d2d;
}

SendButtonTemplatesEditor>QPushButton:pressed#btnOk, SendButtonTemplatesEditor>QPushButton:pressed#btnCancel {
    background-color: #444444;
    border: 1px inset #5b5b5b;
}

/* menu */
QMenu {
    margin: 1px;
    color: #bcbcbc;
    background-color: #212121;
    border: 1px outset #5b5b5b;
    border-radius: 3px;
    padding: 1px;
}

QMenu::disabled, QMenu::item:disabled:selected {
    color: #5b5b5b;
}

QMenu::item {
    padding: 2px 24px 2px 24px;
    border: 1px solid transparent;
}

QMenu::item:selected {
    color: #bcbcbc;
    background-color: #2d2d2d;
}

QMenu::icon {
    background: #212121;
    padding: 1px;
}

QMenu::icon:selected {
    background: #2d2d2d;
}

QMenu::icon:selected:disabled {
    background: #232323;
}

QMenu::icon:checked {
    background: #212121;
}

QMenu::indicator:checked {
    margin-left: 2px;
}

QMenu::indicator:unchecked {
    margin-left: 2px;
}

QMenu::separator {
    background-color: #5b5b5b;
    height: 1px;
}

/* emoji menu - background */
IconSelect {
    background-color: #212121;
}

IconSelectPopup>QScrollArea {
    margin: 0px;
    color: #bcbcbc;
    background-color: #212121;
    border: 1px outset #5b5b5b;
}

/* emoji menu - icons */
IconSelectButton {
    background-color: #212121;
}

/* Last message correction */
QWidget#bottomFrame>QWidget>QTextEdit[correction="true"] {
    background-color: rgb(0,51,0);
}
