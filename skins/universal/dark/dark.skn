/* the whole window */
QMainWindow {
    background: #212121;
    background-color: #212121;
}

/* avatar frame - general settings */
RosterAvatarFrame * {
    border: 1px outset #5b5b5b;
    border-radius: 3px;
}

/* avatar frame - avatar */
QLabel#lb_avatar {
    margin-left: 1px;
    margin-right: -1px;
    margin-bottom: 0px;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}

/* avatar frame - nick */
QLabel#lb_nick {
    margin-bottom: 0px;
    font-size: 12px;
    font-weight: bold;
    color: #bcbcbc;
    background-color: #212121;
    border-radius: 3px;
    border: 1px solid #5b5b5b;
}

/* avatar frame - buttons of PEP and status */
QToolButton#tb_mood, #tb_activity {
    background-color: #2d2d2d;
    border: 1px outset #5b5b5b;
}

QToolButton#tb_mood {
    border-radius: 3px;
    border: 1px outset #5b5b5b;
}

QToolButton#tb_activity {
    border-radius: 3px;
    border: 1px outset #5b5b5b;
}

QToolButton#tb_status {
    margin-top: 3px;
    background-color: #2d2d2d;
    border: 1px outset #5b5b5b;
}

QToolButton#tb_mood:hover, #tb_activity:hover {
    background-color: #5b5b5b;
}

QToolButton#tb_status:hover {
    background-color: #5b5b5b;
}

QToolButton#tb_mood:pressed, #tb_activity:pressed {
    background-color: #212121;
}

QToolButton#tb_status:pressed {
    background-color: #212121;
}

/* avatar frame - button of status, arrow */
/* hack to hide an arrow */
QToolButton#tb_status::menu-indicator {
    image: none;
}

/* avatar frame - field of status message */
QLineEdit#le_status_text {
    margin-left: 0px;
    margin-right: 2px;
    margin-top: 0px;
    margin-bottom: 0px;
    font-size: 13px;
    font-style: italic;
    color: #bcbcbc;
    background-color: #232323;
    border: 1px inset #5b5b5b;
    border-radius: 3px;
    border-bottom-right-radius: 3px;
    min-height: 21px;
}

QLineEdit#le_status_text:hover {
    color: #bcbcbc;
    background-color: #444444;
}

/* frame with contacts - external */
QStackedWidget {
    margin-top: 1px;
    margin-left: 1px;
    margin-right: 1px;
    background-color: #2d2d2d;
    border: 1px inset #5b5b5b;
    border-radius: 3px;
    padding: 2px;
}

/* frame with contacts - internal */
ContactListView {
    background-color: #232323;
    border: 0px;
}

/* frame with contacts - filtered */
PsiFilteredContactListView { }

/* missed events */
QFrame#EventNotifier {
    margin-left: 1px;
    margin-right: 1px;
    color: #bcbcbc;
    background-color: #2d2d2d;
    border: 1px solid #5b5b5b;
    border-radius: 3px;
    padding: 0px
}

EventNotifier>QLabel {
    color: #bcbcbc;
}

/* tool bars (option QToolBar) */
PsiToolBar {
    margin: 2px;
    border: 0px;
}

/* buttons on tool bar */
QToolButton {
    margin: 1px;
    background-color: #2d2d2d;
    border: 1px outset #5b5b5b;
    border-radius: 3px;
    padding: 1px;
}

QToolButton:hover {
    background-color: #5b5b5b;
}

QToolButton:pressed {
    background-color: #212121;
}

QToolButton:checked {
    background-color: #000000;
    border: 1px solid #5b5b5b;
}

/* status button */
PopupActionButton {
    color: #bcbcbc;
    margin-left: 1px;
    margin-right: 1px;
    min-width: 40px;
    max-height: 32px;
    min-height: 24px;
    background-color: #2d2d2d;
    border: 1px outset #5b5b5b;
    border-radius: 3px;
    padding: 0px;
    subcontrol-position: center;
}

PopupActionButton::menu-indicator {
    image: none;
}

PopupActionButton:hover {
    background-color: #5b5b5b;
}

PopupActionButton:hover:pressed {
    background-color: #212121;
}

/* tooltips */
QToolTip {
    color: #bcbcbc;
    background-color: #212121;
    border: 1px solid #2d2d2d;
}

/* scroll bars */
QScrollBar {
    background-color: #212121;
    border: 1px inset #444444;
    border-radius: 7px;
}

QScrollBar:vertical {
    width: 16px;
    margin-left: 1px;
    margin-right: 1px;
}

QScrollBar:horizontal {
    height: 16px;
    margin-top: 1px;
    margin-bottom: 1px;
}

QScrollBar::handle {
    background-color: #2d2d2d;
    border: 1px outset #444444;
    border-radius: 6px;
}

QScrollBar::handle:hover {
    background-color: #5b5b5b;
    border: 1px outset #5b5b5b;
}

/* scroll bar, vertical arrows */
QScrollBar::sub-line:vertical, QScrollBar::add-line:vertical {
    height: 0px;
}

/* scroll bar, horizontal arrows */
QScrollBar::sub-line:horizontal, QScrollBar::add-line:horizontal {
    width: 0px;
}

/* scroll bar - background */
QScrollBar::sub-page, QScrollBar::add-page {
    background: none;
}

/* selected roster element */
QTreeView {
    selection-color: #bcbcbc;
    selection-background-color: #444444;
}

/* menu
ContactListItemMenu { } */

/* menu indicator */
QToolButton::menu-indicator {
    image: none;
}

/* renaming a contact */
QLineEdit {
    color: #bcbcbc;
    background-color: #212121;
    selection-background-color: #2d2d2d;
    border: 1px solid #5b5b5b;
    border-radius: 3px;
    padding: 2px;
}

/* filter */
QStackedWidget>QWidget>QLineEdit {
    color: #bcbcbc;
    background-color: #212121;
}

/* menu */
QMenu {
    margin: 1px;
    color: #bcbcbc;
    selection-color: #2d2d2d;
    background-color: #212121;
    border: 1px solid #5b5b5b;
    border-radius: 3px;
    padding: 1px;
}

QMenu::disabled, QMenu::item:disabled:selected {
    color: #5b5b5b;
}

QMenu::item {
    padding: 2px 24px 2px 24px;
    border: 1px solid transparent;
}

QMenu::item:selected {
    color: #bcbcbc;
    background-color: #2d2d2d;
}

QMenu::icon {
    background: #212121;
    border: 0px;
    padding: 1px;
}

QMenu::icon:selected {
    background: #2d2d2d;
}

QMenu::icon:selected:disabled {
    background: #232323;
}

QMenu::icon:checked {
    background: #212121;
}

QMenu::indicator:checked {
    margin-left: 2px;
}

QMenu::indicator:unchecked {
    margin-left: 2px;
}

QMenu::separator {
    background-color: #5b5b5b;
    height: 1px;
}

QSplitter::handle:horizontal {
    background-color: #2d2d2d;
}

QSplitter::handle:horizontal:hover {
    background-color: #5b5b5b;
}

QSplitter::handle:horizontal:pressed {
    background-color: #202020;
}
