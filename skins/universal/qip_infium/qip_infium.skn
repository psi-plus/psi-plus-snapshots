    border-radius: 3px;
    margin: 2px 4px 2px 4px;
}

QLabel#lb_avatar {
    background-color: #f5faff;
    border-radius: 5px;
}

QToolBar {
    background-color: #f0f0f0;
    border: 0px solid #f0f0f0;
}

QToolButton#tb_status {
    margin: 3px -1px 2px 4px;
    border-top-left-radius: 4px;
    border-top-right-radius: 0px;
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 0px;
    padding-left: 1px;
    icon-size: 14px;
}

QToolButton#tb_activity {
    margin-bottom: 2px;
    margin-top: 3px;
    margin-left: 3px;
    max-width: 23px;
}

QToolButton#tb_mood {
    margin-bottom: 2px;
    margin-top: 3px;
    max-width: 23px;
}

QLineEdit {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #fbfbfb, stop: 0.4 #e9e9e9, stop: 0.5 #e8e8e8, stop: 0.6 #d2d2d2, stop: 1.0 #f7f7f7);
    border: 1px solid #a8a8a8;
    border-top-left-radius: 0px;
    border-top-right-radius: 4px;
    border-bottom-left-radius: 0px;
    border-bottom-right-radius: 4px;
    padding-right: 6px;
    padding-left: 3px;
    margin: 3px 0px 2px 0px;
}

QToolButton::menu-indicator {
    image: none;
}

PopupActionButton::menu-indicator {
    image: none;
}

QStackedWidget>QWidget>QLineEdit {
    border-radius: 4px;
    padding-left: 6px;
    padding-right: 6px;
    margin-right: 10px;
    margin-left: 10px;
    margin-top: 3px;
}

QTreeView {
    alternate-background-color: argb(0,0,0,0);
}
