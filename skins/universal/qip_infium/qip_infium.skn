/* Last message correction */
QWidget#bottomFrame>QWidget>QTextEdit[correction="true"] {
    background-color: rgb(0,51,0);
}

QFrame#log {
    background-color: #f5faff;
    border: 1px solid #a0a0a0;
    border-bottom-color: #a0a0a0;
    border-radius: 1px;
}

QToolButton:pressed {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #9c9c9c, stop: 0.4 #e2e2e2, stop: 0.5 #e4e4e4, stop: 0.6 #dedede, stop: 1.0 #e3e3e3);
    min-height: 1em;
    border: 1px solid #545454;
    border-bottom-color: #545454;
    border-radius: 4px;
}

QToolBar {
    border: 0px solid #ffffff;
}

Q3ListView {
    background-color: #f5faff;
}

QWidget#bottomFrame>QWidget>QTextEdit {
    background-color: #f5faff;
    border: 1px solid #a0a0a0;
    border-bottom-color: #a0a0a0;
    border-radius: 1px;
}

QLineEdit#lb_client {
    background-color: #f0f0f0;
    border: 1px solid #a0a0a0;
    border-bottom-color: #ffffff;
}

QLineEdit#le_topic {
    background-color: #f0f0f0;
    border: 1px solid #a0a0a0;
    border-bottom-color: #ffffff;
}

QLineEdit#le_jid {
    background-color: #f0f0f0;
    border: 1px solid #a0a0a0;
    border-bottom-color: #ffffff;
}

QLabel#lb_ident {
    background-color: #f0f0f0;
    border: 1px solid #a0a0a0;
    border-bottom-color: #ffffff;
}

QLabel#lb_countl {
    background-color: #f0f0f0;
    border: 1px solid #a0a0a0;
    border-bottom-color: #ffffff;
}

QLabel#lb_status {
    background-color: #f0f0f0;
    border: 0px solid #a0a0a0;
    border-bottom-color: #ffffff;
}

QTabBar::tab {
    background-color: #e3e1db;
    border: 2px solid #C4C4C3;
    border-bottom-color: #C2C7CB;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    min-width: 8ex;
    padding: 2px;
}

QTabBar::tab:selected, QTabBar::tab:hover {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #fafafa, stop: 0.4 #f4f4f4, stop: 0.5 #e7e7e7, stop: 1.0 #fafafa);
}

QTabBar::tab:selected {
    border-color: #9B9B9B;
    border-bottom-color: #C2C7CB;
}

QLabel#lb_client {
    border: 0px solid #ffffff;
}

QToolButton::menu-indicator {
    image: none;
}
